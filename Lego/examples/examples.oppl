?p :INDIVIDUAL,
?w :CLASS,
?rt :CLASS
SELECT
?p Type part_of SOME (regulates SOME ?rt)
BEGIN
ADD ?p Type ?w
END;



?f :INDIVIDUAL,
?x :CLASS,
?rt :CLASS
SELECT
?f Type 'kinase activity', ?f Type enabled_by some ?x
BEGIN
ADD ?f Type ?x
END;


?f:INDIVIDUAL, ?x:CLASS, ?rt:CLASS
SELECT ?f Type 'kinase activity',
?f Type enabled_by some ?x,
ASSERTED ?x SubClassOf  protein
 BEGIN ADD ?f Type ?x END;
